{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft.Hosting": "Information",
        "DotnetMultiverse": "Debug"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:G} [{Level:u3}] [{SourceContext}] {Message}{NewLine:1}{Exception:1}"
        }
      }
    ]
  }
}
